<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zy.website.mapper.FilmImageMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.zy.website.model.FilmImageModel">
        <id column="id" property="id" />
        <result column="img_uid" property="imgUid" />
        <result column="img_name" property="imgName" />
        <result column="img_original" property="imgOriginal" />
        <result column="img_status" property="imgStatus" />
        <result column="img_type" property="imgType" />
        <result column="img_suffix" property="imgSuffix" />
        <result column="img_size_level" property="imgSizeLevel" />
        <result column="img_size_plumb" property="imgSizePlumb" />
        <result column="img_remark" property="imgRemark" />
        <result column="creact_time" property="creactTime" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, img_uid, img_name, img_original, img_status, img_type, img_suffix, img_size_level, img_size_plumb, img_remark, creact_time
    </sql>

</mapper>
